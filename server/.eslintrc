{
  "root": true,
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "airbnb-typescript/base",
    "prettier"
  ],
  "parserOptions": {
    "project": "**/tsconfig.json"
  },
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],
  "ignorePatterns": ["./node_modules/"],
  "rules": {
    "class-methods-use-this": 0
  }
}